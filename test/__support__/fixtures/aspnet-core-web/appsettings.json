{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HealthChecks": {
    "UI": {
      "HealthChecksUIEnabled": true,
      "HealthChecksUI": {
        "Enabled": true,
        "EvaluationTimeInSeconds": 30,
        "UnHealthyStatus": "503"
      }
    }
  }
}